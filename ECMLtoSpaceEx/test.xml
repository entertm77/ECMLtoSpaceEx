<?xml version="1.0" encoding="iso-8859-1"?>
<sspaceex xmlns="http://www-verimag.imag.fr/xml-namespaces/sspaceex" version="0.2" math="SpaceEx">
  <component id="Rotor2">
    <param name="PowerIncreasingRate" type="real" local="true" d1="1" d2="1" dynamics="const" />
    <param name="Thrust2" type="real" local="false" d1="1" d2="1" dynamics="any" />
    <param name="PowerDecreasingRate" type="real" local="true" d1="1" d2="1" dynamics="const" />
    <param name="ThrustControl2" type="real" local="false" d1="1" d2="1" dynamics="any" />
    <param name="MaximumPower" type="real" local="true" d1="1" d2="1" dynamics="const" />
    <param name="Power" type="real" local="true" d1="1" d2="1" dynamics="const" />
    <location id="2" name="Holding_1">
      <invariant>ThrustControl2&gt;=(Power/MaximumPower)&amp;ThrustControl2&lt;=(Power/MaximumPower)</invariant>
      <flow>Power'==0</flow>
    </location>
    <location id="3" name="Incrasing_1">
      <invariant>ThrustControl2&gt;=(Power/MaximumPower)</invariant>
      <flow>Power'==PowerIncreasingRate</flow>
    </location>
    <location id="4" name="Decreasing_1">
      <invariant>ThrustControl2&gt;=(Power/MaximumPower)</invariant>
      <flow>Power'==PowerDecreasingRate</flow>
    </location>
    <transition source="2" target="3">
      <guard>ThrustControl2&gt;(Power/MaximumPower)</guard>
      <assignment>Thrust2:=Power</assignment>
    </transition>
    <transition source="4" target="2">
      <guard>ThrustControl2&lt;=(Power/MaximumPower)</guard>
      <assignment>Thrust2:=Power</assignment>
    </transition>
    <transition source="2" target="4">
      <guard>ThrustControl2&lt;(Power/MaximumPower)</guard>
      <assignment>Thrust2:=Power</assignment>
    </transition>
    <transition source="3" target="2">
      <guard>ThrustControl2&lt;=(Power/MaximumPower)</guard>
      <assignment>Thrust2:=Power</assignment>
    </transition>
  </component>
  <component id="overall_network1">
    <bind component="timing_automaton" as="timed_component" />
    <bind component="Rotor2" as="Rotor2" />
  </component>
</sspaceex>

